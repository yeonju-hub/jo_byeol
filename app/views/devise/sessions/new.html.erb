<div id="wrap">
	
	<div class="log_in">
		
	
		<h2>로그인</h2><br />

		<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
		  <div class="field">
			이메일<br />
			<%= f.email_field :email, autofocus: true, autocomplete: "email" %><br /><br />
		  </div>

		  <div class="field">
			비밀번호<br />
			<%= f.password_field :password, autocomplete: "current-password" %><br /><br />
		  </div>

		  <% if devise_mapping.rememberable? %>
			<div class="field">
			  <%= f.check_box :remember_me %>
			  자동 로그인<br /><br />
			</div>
		  <% end %>

		  <div class="actions">
			<%= f.submit "로그인" %>
		  </div>
		<% end %>

		<%= render "devise/shared/links" %>
	</div>
</div>