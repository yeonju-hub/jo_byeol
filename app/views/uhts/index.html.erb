<p id="notice"><%= notice %></p>

<h1>Uhts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uhts.each do |uht| %>
      <tr>
        <td><%= uht.user_id %></td>
        <td><%= uht.team_id %></td>
        <td><%= link_to 'Show', uht %></td>
        <td><%= link_to 'Edit', edit_uht_path(uht) %></td>
        <td><%= link_to 'Destroy', uht, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
	  
	  <% @team.each do |team| %>
	  	<%= team.t_name %>
	  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uht', new_uht_path %>
